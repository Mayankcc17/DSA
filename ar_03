// 3. WAP to sum of Array Elements.

#include <iostream>
using namespace std;

int main()
{
    int arr[4] = {1, 2, 3, 4};

    cout << "Elements of Array : ";
    for (int i = 0; i < 4; i++)
    {
        cout << arr[i] << " ";
    }

    cout << endl << "Sum of elements of Array : ";

    int sum = 0;
    for (int i = 0; i < 4; i++)
    {
        sum = sum + arr[i];
    }
    cout << sum;

    return 0;
}
